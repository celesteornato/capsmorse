CompileFlags:
  Add: [
  -xc,
  -std=gnu23,
  -Wall,
  -Wextra,
  -Wpedantic,
  -Wunused,
  -Wfloat-equal,
  -Wundef,
  -Wshadow,
  -Wpointer-arith,
  -Wcast-align,
  -Wstrict-prototypes,
  -Wstrict-overflow=5,
  -Wwrite-strings,
  -Wcast-qual,
  -Wswitch-default,
  -Wswitch-enum,
  -Wconversion,
  -Wvla,
  -Wunreachable-code,
  ]
